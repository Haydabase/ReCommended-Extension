using System;
using System.Diagnostics.Contracts;

namespace Test
{
    internal class ExecuteWithoutZero
    {
        [Flags]
        enum NumbersWithoutZero
        {
            One = 1,
            Two = 2,
            Four = 4,
            Eight = 8,
        }

        void Available(NumbersWithoutZero two{caret})
        {
          Contract.Requires(two >= 0 && two <= (NumbersWithoutZero.One | NumbersWithoutZero.Two |
                                                NumbersWithoutZero.Four | NumbersWithoutZero.Eight));
        }
    }
}