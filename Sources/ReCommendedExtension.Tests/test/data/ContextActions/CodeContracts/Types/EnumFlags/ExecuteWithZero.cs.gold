using System;
using System.Diagnostics.Contracts;

namespace Test
{
    internal class ExecuteWithZero
    {
        [Flags]
        enum Numbers
        {
            None = 0,
            One = 1,
            Two = 2,
            Four = 4,
        }

        void Method(Numbers one{caret})
        {
          Contract.Requires(one >= Numbers.None && one <= (Numbers.One | Numbers.Two | Numbers.Four));
        }
    }
}